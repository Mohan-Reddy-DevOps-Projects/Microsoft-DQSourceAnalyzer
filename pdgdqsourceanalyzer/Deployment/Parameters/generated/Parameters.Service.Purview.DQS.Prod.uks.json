          "/subscriptions/34a8ddf8-ebee-475d-97d3-494d34c800b7/resourceGroups/babylon-gw-infra-prod-rg-eus/providers/Microsoft.ManagedIdentity/userAssignedIdentities/Babylon_Ev2Shell_ManagedIdentity_Prod"
